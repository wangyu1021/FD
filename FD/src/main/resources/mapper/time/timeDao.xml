<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fd.dao.TimeDao">
    <select id="findAllTime" resultType="com.fd.pojo.Time">
        select id,time from time;
    </select>
    <select id="findTimeById" resultType="com.fd.pojo.Time">
        select id,time from time where id=#{id}
    </select>

</mapper>

