<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fd.dao.MotionTypeDao">
    <select id="findAllMotionType" resultType="com.fd.pojo.MotionType">
        select id , name from motiontype
    </select>
    <select id="findMotionTypeById" resultType="com.fd.pojo.MotionType">
        select id , name from motiontype where id=#{id}
    </select>
    <select id="findMotionTypeByName" resultType="com.fd.pojo.MotionType">
        select id , name from motiontype where name=#{name}
    </select>

</mapper>

